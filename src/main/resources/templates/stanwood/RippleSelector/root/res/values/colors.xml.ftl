<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="${colorName}">${color}</color>
<#if usePressedColor>
    <color name="${pressedColorName}">${pressedColor}</color>
</#if>
<#if useSelectedColor>
    <color name="${selectedColorName}">${selectedColor}</color>
</#if>
<#if useDisabledColor>
    <color name="${disabledColorName}">${disabledColor}</color>
</#if>
</resources>